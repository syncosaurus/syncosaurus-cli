{
  "name": "syncosaurus-cli",
  "version": "0.6.0",
  "description": "Syncosaurus CLI to create and manage Syncosaurus backed React applications.",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/syncosaurus/syncosaurus-cli",
  "bugs": "https://github.com/syncosaurus/syncosaurus-cli/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syncosaurus/syncosaurus-cli.git"
  },
  "license": "MIT",
  "author": "team2ls",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "syncosaurus": "bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && npm run build:templates",
    "build:templates": "shx mkdir dist/templates && shx cp -r ./src/templates/* dist/templates",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "syncosaurus",
    "commands": "./dist/commands",
    "dirname": "syncosaurus",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^4.3.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "@types/inquirer": "^9.0.7",
    "chalk": "^5.3.0",
    "execa": "^8.0.1",
    "ora": "^8.0.1",
    "wrangler": "^3.47.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.6.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
